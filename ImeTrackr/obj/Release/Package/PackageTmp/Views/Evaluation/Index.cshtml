@model IEnumerable<ImeTrackr.Models.Evaluation>

@{
    ViewBag.Title = "Evaluations";
}

<h2>Evaluation</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Plaintiff 
        </th>
        <th>
            Contact
        </th>
        <th>
            Notes
        </th>
        <th>
            Day One
        </th>
        <th>
            Day Two
        </th>
        <th>
            Case Number
        </th>
        <th>
            Case Name
        </th>
        <th>Complete?</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Plaintiff.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contact.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Notes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DayOne)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DayTwo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CaseNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CaseName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsComplete)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>